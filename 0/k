FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 8e-2;

boundaryField
{
    window_extruded
    {
        type            zeroGradient;
    }

    fanAbove_extruded
    {
        type            kqRWallFunction;
        value           $internalField;      
    }

    fan_extruded
    {
        type            fixedValue;
        value           $internalField;
    }

    door_extruded
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    walls_extruded
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //
