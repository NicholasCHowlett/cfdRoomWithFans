FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    walls_extruded
    {
        type            noSlip;
    }

    fan_extruded
    {
        type            fixedValue;
        value           uniform (2.43 -2.43 0);
    }

    door_extruded
    {
        type            zeroGradient;
    }

    window_extruded
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            empty;
    }
}


