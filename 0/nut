FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls_extruded
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    fan_extruded
    {
        type            calculated;
        value           uniform 0;
    }

    door_extruded
    {
        type            calculated;
        value           uniform 0;
    }

    window_extruded
    {
        type            calculated;
        value           uniform 0;
    }

    defaultFaces
    {
        type            empty;
    }
}


